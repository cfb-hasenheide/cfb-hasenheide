= content_for(:title, "Spielbericht #{@event.name}")

= render 'events/subnavigation', event: @event

.page-header
  %h1
    Spielbericht
    %small= @event.name

= render 'events/basic_information', event: @event

= render 'report',
  event: @event,
  report: @report,
  attending_players: @attending_players,
  watching_players: @watching_players
